<content-login />

<script>
(function (self) {
	if (get_cache (backend_path + '/login') && ! get_cache (backend_path + '/login') .error) {
		window .location .hash = '#service'
	}
	
	util .register (self);
	util .login (self);
	
	display_errors ('register', self);
	display_errors ('login', self);
	
})(this);
</script>

